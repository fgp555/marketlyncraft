<header>
  <h2><a href="/">Logo</a></h2>
  <nav>
    <% if (user) { %>
    <a href="/">Home</a>
    <a href="/download">Download</a>
    <a href="/videos">Videos</a>
    <a href="/profile">
      <img src="<%= user.photos[0].value %>" alt="Profile Image" width="24" />
      <span><%= user.name.givenName %></span>
    </a>
    <a href="/logout" class="btn">Logout</a>

    <% } else { %>
    <a href="/auth/google" class="btn">Login with Google</a>
    <% } %>
  </nav>
</header>
